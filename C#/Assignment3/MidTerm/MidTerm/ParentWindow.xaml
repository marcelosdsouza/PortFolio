<Window x:Class="MidTerm.ParentWindow"
        Name ="parentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MidTerm"
        mc:Ignorable="d"
        Title="Parent Window" Height="330" Width="330" Closed="ParentWindow_Closed">
    <Grid>
        <Button Name="ChildFromParent" Content="Spawn Child Window" HorizontalAlignment="Center" VerticalAlignment="Center" Click="ChildFromParent_Click"/>
    </Grid>
</Window>
